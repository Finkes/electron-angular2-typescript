{
	"version": "0.2.0",
	"configurations": [
		{
			"preLaunchTask": "build",
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "app/server/index.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": ".",
			"runtimeExecutable": "./node_modules/.bin/electron",
			"runtimeArgs": [
				"./app/server"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": "bin"
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}